{
  "name": "webpack-express-typescript",
  "version": "0.0.1",
  "description": "Easy starter project for writing modern Express Applications using TypeScript, and Webpack",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "server:dev:nodemon": "NODE_ENV=development nodemon --watch '**/*.ts' --ignore '**/*.spec.ts' --exec 'ts-node' bin/www.ts",
    "build": "ts-node node_modules/webpack/bin/webpack.js --config config/webpack.config.ts",
    "compiled": "ts-node build/compiled",
    "clean": "rimraf build",
    "start": "yarn run server:dev:nodemon"
  },
  "main": "",
  "keywords": [
    "Easy",
    "TypeScript",
    "Starter",
    "Template"
  ],
  "author": "Vladimir Saakyan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.3",
    "ejs": "^2.5.1",
    "errorhandler": "^1.5.0",
    "express": "^4.15.2",
    "method-override": "^2.3.10",
    "mongodb": "^3.0.1",
    "mongoose": "^5.0.0",
    "morgan": "^1.7.0",
    "nconf": "^0.10.0",
    "node": "^9.3.0",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^0.0.30",
    "@types/mime": "^2.0.0",
    "@types/mongodb": "^3.0.2",
    "@types/mongoose": "^4.7.32",
    "@types/nconf": "^0.0.36",
    "@types/winston": "^2.3.7",
    "nodemon": "^1.14.10",
    "ts-loader": "^2.0.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.2.1",
    "typings": "^1.3.3",
    "webpack": "^2.2.1"
  }
}